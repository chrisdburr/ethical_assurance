@article{przybylski2013,
  title = {Motivational, Emotional, and Behavioral Correlates of Fear of Missing Out},
  author = {Przybylski, Andrew K. and Murayama, Kou and DeHaan, Cody R. and Gladwell, Valerie},
  year = {2013},
  month = jul,
  journal = {Computers in Human Behavior},
  volume = {29},
