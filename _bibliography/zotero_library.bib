@article{ruch2019,
  title = {Trends in {{Suicide Among Youth Aged}} 10 to 19 {{Years}} in the {{United States}}, 1975 to 2016},
  author = {Ruch, Donna A. and Sheftall, Arielle H. and Schlagbaum, Paige and Rausch, Joseph and Campo, John V. and Bridge, Jeffrey A.},
  year = {2019},
