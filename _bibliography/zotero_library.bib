  shorttitle = {Covid-19 and the Cardiovascular System},
  author = {Azevedo, Rafael Bellotti and Botelho, Bruna Gopp and de Hollanda, Jo{\~a}o Victor Gon{\c c}alves and Ferreira, Leonardo Villa Le{\~a}o and {Junqueira de Andrade}, Let{\'i}cia Zarur and Oei, Stephanie Si Min Lilienwald and Mello, Tom{\'a}s de Souza and Muxfeldt, Elizabeth Silaid},
